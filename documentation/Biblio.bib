@ARTICLE{Chester.NIMA.2006,
  author = {A. Chester and P. Adrich and A. Becerril and D. Bazin and C.M. Campbell
	and J.M. Cook and D.-C. Dinca and W.F. Mueller and D. Miller and
	V. Moeller and R.P. Norris and M. Portillo and K. Starosta and A.
	Stolz and J.R. Terry and H. Zwahlen and C. Vaman and A. Dewald},
  title = {Application of the time-of-flight technique for lifetime measurements
	with relativistic beams of heavy nuclei},
  journal = {NIM A},
  year = {2006},
  volume = {562},
  pages = {230 - 240},
  number = {1},
  abstract = {A novel method for picosecond lifetime measurements of excited [gamma]-ray
	emitting nuclear states has been developed for fast beams from fragmentation
	reactions. A test measurement was carried out with a beam of 124Xe
	at an energy of . The beam ions were Coulomb excited to the state
	on a movable target. Excited nuclei emerged from the target and decayed
	in flight after a distance related to the lifetime. A stationary
	degrader positioned downstream with respect to the target was used
	to further reduce the velocity of the excited nuclei. As a consequence,
	the [gamma]-ray decays from the excited state that occurred before
	or after traversing the degrader were measured at a different Doppler
	shift. The [gamma]-ray spectra were analyzed from the forward ring
	of the Segmented Germanium Array; this ring positioned at 37o simultaneously
	provides the largest sensitivity to changes in [beta] and the best-energy
	resolution. The ratio of intensities in the peaks at different Doppler
	shifts gives information about the lifetime if the velocity [beta]
	is measured. The results and range of the application of the method
	are discussed.},
  doi = {DOI: 10.1016/j.nima.2006.02.182},
  file = {Chester.NIMA.2006.pdf:Chester.NIMA.2006.pdf:PDF},
  issn = {0168-9002},
